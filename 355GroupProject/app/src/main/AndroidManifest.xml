<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="edu.vcu.cmsc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.user.LoginActivity"
            android:label="@string/login.title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.user.RegisterActivity"
            android:label="@string/register.title">
        </activity>
        <activity android:name=".ui.catalog.CatalogActivity">
        </activity>
        <activity android:name=".ui.catalog.CatalogEntryActivity">
        </activity>
    </application>

</manifest>